{
  "manifest_version": 3,
  "name": "History Downloader",
  "version": "1.0",
  "description": "Download and save your Chrome browsing history",
  "permissions": [
    "history",
    "downloads"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
